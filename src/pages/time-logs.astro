---
import AppLayout from "../layouts/AppLayout.astro";
import { TimeLogsViewWrapper } from "../components/time-logs/TimeLogsViewWrapper";

// Get user from middleware (can be null if not logged in)
const user = Astro.locals.user;
---

<AppLayout title="Czas pracy - DailyPath" user={user}>
  <TimeLogsViewWrapper client:load initialUser={user} />
</AppLayout>

